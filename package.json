    "sirv": "^0.4.2",
    "syllable": "^4.0.0"
  },
  "devDependencies": {
    "node-sass": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "sapper": "^0.27.11",
    "sass": "^1.22.12",
